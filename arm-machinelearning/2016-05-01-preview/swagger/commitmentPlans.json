    "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MachineLearning/commitmentPlans/{planName}/usageHistory": {
      "get": {
        "tags": [
          "CommitmentPlans"
        ],
        "operationId": "CommitmentPlans_GetUsageHistory",
        "description": "Retrieves the usage history for an Azure ML commitment plan.",
        "parameters": [
          {
           "$ref": "#/parameters/SubscriptionIdParameter"
