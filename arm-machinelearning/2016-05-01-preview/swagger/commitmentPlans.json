            "name": "planName",
            "description": "The association's parent commitment plan name.",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "associationName",
            "description": "The commitment association name.",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "movePayload",
            "description": "The move request payload.",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/MoveCommitmentAssociationRequest"
            }
          },
          {
            "$ref": "#/parameters/APIVersionParameter"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/CommitmentAssociation"
            }
          }
        }
      }
    },
    "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MachineLearning/commitmentPlans/{planName}": {
