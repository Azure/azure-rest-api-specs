        "parameters": [
          {
            "name": "patchPayload",
            "description": "The payload to patch the Azure ML commitment plan with.",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/CommitmentPlan"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
