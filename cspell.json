    ],
    "ignorePaths": [
        "node_modules/**",
        "**/examples/**",
        "**/package-lock.json",
        "cspell.json"
    ],
    "ignoreRegExpList": [
        "v\\d",
        "email",
        "Base64"
    ],
    "minWordLength": 4,
    "dictionaries": [
        "companies",
        "csharp",
        "custom-words",
        "go",
        "html",
        "java",
        "node",
        "python",
        "softwareTerms",
        "typescript"
    ],
    "enableFiletypes": [
        "cadl"
    ], 
    "words": [
        "mgmt",
        "typespec"
    ]
}
