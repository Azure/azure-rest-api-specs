{
  "name": "azure-notificationhubs-typespec",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build:openapi3": "tsp compile . --emit @typespec/openapi3",
    "build:csharp": "tsp compile . --emit @azure-tools/typespec-csharp",
    "build:java": "tsp compile . --emit @azure-tools/typespec-java",
    "build:python": "tsp compile . --emit @azure-tools/typespec-python",
    "build:ts": "tsp compile . --emit @azure-tools/typespec-ts"
  },
  "dependencies": {
    "@typespec/compiler": "latest",
    "@typespec/rest": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/versioning": "latest",
    "@azure-tools/typespec-autorest": "latest",
    "@azure-tools/typespec-azure-core": "latest",
    "@azure-tools/typespec-csharp": "latest",
    "@azure-tools/typespec-java": "latest",
    "@azure-tools/typespec-python": "latest",
    "@azure-tools/typespec-ts": "latest"
  },
  "private": true
}
