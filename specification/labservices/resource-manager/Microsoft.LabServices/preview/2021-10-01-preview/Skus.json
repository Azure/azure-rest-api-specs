{
  "swagger": "2.0",
  "info": {
    "version": "2021-10-01-preview",
    "title": "LabServicesClient",
    "description": "REST API for managing Azure Lab Services VM size data."
  },
  "host": "management.azure.com",
  "schemes": ["https"],
