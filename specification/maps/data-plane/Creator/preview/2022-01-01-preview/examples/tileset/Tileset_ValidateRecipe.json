{
  "parameters": {
    "geography": "us",
    "api-version": "2022-01-01-preview",
    "datasetId": "[datasetId]",
    "TilesetRecipe": {
      "version": 1,
      "layers": {
        "Indoor unit": {
          "minzoom": 10,
          "maxzoom": 18,
          "features": {
            "featureClasses": [ "unit", "wall" ],
            "filter": [
              "any",
              [
                "!=",
                [ "get", "roomType" ],
                "conference"
              ],
              [
                "!=",
                [ "get", "roomType" ],
                "restroom"
              ]
            ]
          }
        }
      }
    }
  },
  "responses": {
    "200": {
      "body": {
        "valid": false,
        "warnings": [],
        "errors": [
          "minzoom 12 cannot be larger than maxzoom 11 for Indoor unit layer."
        ]       
      }  
    }
  }
}
