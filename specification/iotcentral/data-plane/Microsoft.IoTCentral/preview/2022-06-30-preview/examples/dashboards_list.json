                    "xAxisEnabled": true,
                    "yAxisEnabled": true,
                    "legendEnabled": true
                  },
                  "capabilities": [
                    {
                      "capability": "componentName.telemetryName",
                      "aggregateFunction": "avg"
                    }
                  ],
                  "queryRange": {
                    "type": "time",
                    "duration": "P1W",
                    "resolution": "PT12H"
                  }
                },
                "x": 0,
                "y": 0,
                "width": 3,
                "height": 3
              }
            ],
            "favorite": false
          }
        ]
      }
    }
