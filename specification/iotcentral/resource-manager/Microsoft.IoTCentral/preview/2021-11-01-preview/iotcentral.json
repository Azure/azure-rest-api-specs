      "type": "object",
      "properties": {
        "applyToHub": {
          "description": "Whether these rules apply to the internal IoT Hub.",
          "type": "boolean"
        },
        "defaultAction": {
          "description": "The default network action to apply.",
          "$ref": "#/definitions/NetworkAction"
        },
        "ipRules": {
          "description": "List of IP rules.",
          "type": "array",
          "items": {
            "$ref": "#/definitions/NetworkRuleSetIpRule"
          }
        }
      }
    },
    "NetworkRuleSetIpRule": {
