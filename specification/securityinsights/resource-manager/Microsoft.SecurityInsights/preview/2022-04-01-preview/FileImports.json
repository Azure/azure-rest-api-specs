  "definitions": {
    "FileMetadata": {
      "description": "Represents a file.",
      "properties": {
        "fileFormat": {
          "description": "The format of the file",
          "enum": [
            "CSV",
            "JSON",
            "XML",
            "Other"
          ],
          "type": "string",
          "x-ms-enum": {
            "modelAsString": true,
            "name": "fileFormat",
            "values": [
              {
                "description": "A CSV file.",
                "value": "CSV"
              },
              {
                "description": "A JSON file.",
                "value": "JSON"
              },
              {
                "description": "An XML file.",
                "value": "XML"
              },
              {
                "description": "A file of other format.",
                "value": "Other"
              }
            ]
          }
        },
        "fileName": {
          "description": "The name of the file.",
          "type": "string"
        },
        "fileSize": {
          "description": "The size of the file.",
          "format": "int32",
          "type": "integer"
        },
        "fileContentUri": {
          "description": "A URI with a valid SAS token to allow uploading / downloading the file.",
          "type": "string"
        },
        "isDeleted": {
          "description": "Indicates wwhether the backing file was deleted from the storge account.",
          "type": "boolean"
        }
      },
      "type": "object"
    },
    "FileImport": {
      "allOf": [
        {
          "$ref": "../../../common/1.0/types.json#/definitions/Resource"
        }
      ],
