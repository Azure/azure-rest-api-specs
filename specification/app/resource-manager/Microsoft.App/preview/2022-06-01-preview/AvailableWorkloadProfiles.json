  "definitions": {
    "AvailableWorkloadProfile": {
      "description": "A premium workload profile.",
      "type": "object",
      "allOf": [
        {
          "$ref": "../../../../../common-types/resource-management/v3/types.json#/definitions/ProxyResource"
        }
      ],
      "properties": {
        "location": {
