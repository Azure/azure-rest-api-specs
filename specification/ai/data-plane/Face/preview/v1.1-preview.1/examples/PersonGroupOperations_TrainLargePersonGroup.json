  },
  "responses": {
    "202": {
      "headers": {
        "operation-Location": "https://contoso.com/operationstatus"
      }
    },
    "401": {
      "body": {
        "error": {
          "code": "Unspecified",
          "message": "Access denied due to invalid subscription key. Make sure you are subscribed to an API you are trying to call and provide the right key."
        }
      }
    },
