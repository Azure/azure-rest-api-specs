{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "CreateConversationRequest.json",
    "type": "object",
    "properties": {
        "metadata": {
            "anyOf": [
                {
                    "$ref": "#/$defs/Metadata"
                },
                {
                    "type": "null"
                }
            ]
        },
        "items": {
            "anyOf": [
                {
                    "type": "array",
                    "items": {
                        "$ref": "#/$defs/InputItem"
                    }
                },
                {
                    "type": "null"
                }
            ]
        }
    },
    "$defs": {
        "Metadata": {
            "type": "object",
            "properties": {},
            "unevaluatedProperties": {
                "type": "string"
            },
            "description": "Set of 16 key-value pairs that can be attached to an object. This can be\nuseful for storing additional information about the object in a structured\nformat, and querying for objects via API or the dashboard.\nKeys are strings with a maximum length of 64 characters. Values are strings\nwith a maximum length of 512 characters."
        },
        "InputItem": {
            "type": "object",
            "properties": {
                "type": {
                    "$ref": "#/$defs/InputItemType"
                }
            },
            "required": [
                "type"
            ],
            "oneOf": [
                {
                    "$ref": "#/$defs/EasyInputMessage"
                },
                {
                    "$ref": "#/$defs/ItemReferenceParam"
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string",
                            "not": {
                                "enum": [
                                    "message",
                                    "item_reference"
                                ]
                            }
                        }
                    },
                    "required": [
                        "type"
                    ]
                }
            ]
        },
        "EasyInputMessage": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "message",
                    "description": "The type of the message input. Always `message`."
                },
                "role": {
                    "anyOf": [
                        {
                            "type": "string",
                            "const": "user"
                        },
                        {
                            "type": "string",
                            "const": "assistant"
                        },
                        {
                            "type": "string",
                            "const": "system"
                        },
                        {
                            "type": "string",
                            "const": "developer"
                        }
                    ],
                    "description": "The role of the message input. One of `user`, `assistant`, `system`, or\n  `developer`."
                },
                "content": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "$ref": "#/$defs/InputMessageContentList"
                        }
                    ],
                    "description": "Text, image, or audio input to the model, used to generate a response.\n  Can also contain previous assistant responses."
                }
            },
            "required": [
                "type",
                "role",
                "content"
            ],
            "description": "A message input to the model with a role indicating instruction following\nhierarchy. Instructions given with the `developer` or `system` role take\nprecedence over instructions given with the `user` role. Messages with the\n`assistant` role are presumed to have been generated by the model in previous\ninteractions.",
            "title": "Input message"
        },
        "ItemReferenceParam": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "item_reference",
                    "default": "item_reference",
                    "description": "The type of item to reference. Always `item_reference`."
                },
                "id": {
                    "type": "string",
                    "description": "The ID of the item to reference."
                }
            },
            "required": [
                "type",
                "id"
            ],
            "description": "An internal identifier for an item to reference.",
            "title": "Item reference"
        },
        "InputItemType": {
            "anyOf": [
                {
                    "type": "string"
                },
                {
                    "type": "string",
                    "const": "message"
                },
                {
                    "type": "string",
                    "const": "item_reference"
                }
            ]
        },
        "InputMessageContentList": {
            "type": "array",
            "items": {
                "$ref": "#/$defs/InputContent"
            },
            "description": "A list of one or many input items to the model, containing different content\ntypes.",
            "title": "Input item content list"
        },
        "InputContent": {
            "type": "object",
            "properties": {
                "type": {
                    "$ref": "#/$defs/InputContentType"
                }
            },
            "required": [
                "type"
            ],
            "oneOf": [
                {
                    "$ref": "#/$defs/InputContentInputTextContent"
                },
                {
                    "$ref": "#/$defs/InputContentInputImageContent"
                },
                {
                    "$ref": "#/$defs/InputContentInputFileContent"
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string",
                            "not": {
                                "enum": [
                                    "input_text",
                                    "input_image",
                                    "input_file"
                                ]
                            }
                        }
                    },
                    "required": [
                        "type"
                    ]
                }
            ]
        },
        "InputContentInputTextContent": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "input_text",
                    "default": "input_text",
                    "description": "The type of the input item. Always `input_text`."
                },
                "text": {
                    "type": "string",
                    "description": "The text input to the model."
                }
            },
            "required": [
                "type",
                "text"
            ],
            "description": "A text input to the model.",
            "title": "Input text"
        },
        "InputContentInputImageContent": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "input_image",
                    "default": "input_image",
                    "description": "The type of the input item. Always `input_image`."
                },
                "image_url": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "file_id": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "detail": {
                    "$ref": "#/$defs/ImageDetail",
                    "description": "The detail level of the image to be sent to the model. One of `high`, `low`, or `auto`. Defaults to `auto`."
                }
            },
            "required": [
                "type",
                "detail"
            ],
            "description": "An image input to the model. Learn about [image inputs](https://platform.openai.com/docs/guides/vision).",
            "title": "Input image"
        },
        "InputContentInputFileContent": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "input_file",
                    "default": "input_file",
                    "description": "The type of the input item. Always `input_file`."
                },
                "file_id": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "filename": {
                    "type": "string",
                    "description": "The name of the file to be sent to the model."
                },
                "file_url": {
                    "type": "string",
                    "description": "The URL of the file to be sent to the model."
                },
                "file_data": {
                    "type": "string",
                    "description": "The content of the file to be sent to the model."
                }
            },
            "required": [
                "type"
            ],
            "description": "A file input to the model.",
            "title": "Input file"
        },
        "InputContentType": {
            "anyOf": [
                {
                    "type": "string"
                },
                {
                    "type": "string",
                    "const": "input_text"
                },
                {
                    "type": "string",
                    "const": "input_image"
                },
                {
                    "type": "string",
                    "const": "input_file"
                }
            ]
        },
        "ImageDetail": {
            "type": "string",
            "enum": [
                "low",
                "high",
                "auto"
            ]
        }
    }
}