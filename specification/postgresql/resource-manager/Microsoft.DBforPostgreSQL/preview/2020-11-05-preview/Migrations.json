          "type": "string"
        },
        "migrationDetailsLevel": {
          "$ref": "#/definitions/MigrationDetailsLevel"
        },
        "currentStatus": {
          "$ref": "#/definitions/MigrationStatus"
        },
        "sourceDBServerMetadata": {
