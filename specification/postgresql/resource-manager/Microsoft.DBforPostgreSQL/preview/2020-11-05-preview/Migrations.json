    "AADApp": {
      "type": "object",
      "required": ["aadSecret", "clientId", "tenantId"],
      "properties": {
        "aadSecret": {
          "type": "string",
          "x-ms-secret": true,
          "x-ms-external": true
        },
        "clientId": {
          "type": "string",
          "format": "uuid"
        },
        "tenantId": {
          "type": "string",
          "format": "uuid"
        }
      },
      "description": "Azure active directory application."
