        "supportedThroughput": {
          "type": "integer",
          "format": "int32",
          "description": "Minimum supported throughput (in MB/s) of storage.",
          "readOnly": true
        },
        "supportedMaximumThroughput": {
          "type": "integer",
          "format": "int32",
          "description": "Maximum supported throughput (in MB/s) of storage.",
          "readOnly": true
        },
        "defaultIopsTier": {
          "type": "string",
          "description": "Default IOPS for this tier and storage size.",
          "readOnly": true
