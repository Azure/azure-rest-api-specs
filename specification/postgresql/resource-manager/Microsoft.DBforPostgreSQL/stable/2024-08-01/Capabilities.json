        "defaultStorageSizeMb": {
          "type": "integer",
          "format": "int64",
          "description": "Default storage size (in MB) for this storage tier.",
          "readOnly": true
        },
        "supportedStorageMb": {
          "type": "array",
          "description": "Configurations of storage supported for this storage tier.",
