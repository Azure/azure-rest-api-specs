        }
      },
      "patch": {
        "tags": [
          "Extensions",
          "ClusterExtensions"
        ],
        "description": "Patch an existing Kubernetes Cluster Extension.",
        "operationId": "Extensions_Update",
        "x-ms-examples": {
          "Update Extension": {
            "$ref": "./examples/PatchExtension.json"
          }
        },
