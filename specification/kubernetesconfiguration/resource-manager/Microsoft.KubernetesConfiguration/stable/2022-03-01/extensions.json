        ],
        "operationId": "OperationStatus_Get",
        "description": "Get Async Operation status",
        "x-ms-examples": {
          "ExtensionAsyncOperationStatus Get": {
            "$ref": "./examples/GetExtensionAsyncOperationStatus.json"
          }
        },
        "parameters": [
