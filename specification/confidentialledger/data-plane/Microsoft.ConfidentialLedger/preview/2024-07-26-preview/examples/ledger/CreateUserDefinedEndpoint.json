/*

content_endpoint_def = {
  "get": self._endpoint_properties(js_module="test.js",
  js_function="content")
}

self.modules = [
  {
    "name": "test.js",
    "module": TESTJS
  },
]
*/

{
  "put": {
    "parameters": [
      {
        "description": "User definied endpoint.",
        "in": "body",
        "name": "userDefinedEndpoint",
        "required": true,
        "schema": {
          "description": "User defined endpoints",
          "properties": {
            "bundle": {
              "metadata": { "endpoints": { "/content": "content_endpoint_def" } },
              "modules": "self.modules"
            }
          },
          "type": "object"
        }
      }
    ],
    "responses": {
      "201": {
        "description": "The custom endpoint.",
        "schema": {
          "bundle": {
            "metadata": { "endpoints": { "/content": "content_endpoint_def" } },
            "modules": "self.modules"
          }
        }
      }
    }
  }
}
}
