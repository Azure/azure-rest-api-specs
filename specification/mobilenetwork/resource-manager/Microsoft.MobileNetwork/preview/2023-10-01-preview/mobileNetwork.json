          "$ref": "./ts29571.json#/definitions/PlmnId",
          "description": "The unique public land mobile network identifier for the network. This is made up of the mobile country code and mobile network code, as defined in https://www.itu.int/rec/T-REC-E.212. The values 001-01 and 001-001 can be used for testing and the values 999-99 and 999-999 can be used on internal private networks."
        },
        "publicLandMobileNetworkIdentifiers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/publicLandMobileNetworkIdentifier"
          },
          "minItems": 1,
          "uniqueItems": true,
          "description": "A list of public land mobile network identifiers. This is made up of the mobile country code and mobile network code, as defined in https://www.itu.int/rec/T-REC-E.212. The values 001-01 and 001-001 can be used for testing and the values 999-99 and 999-999 can be used on internal private networks. Each identifier can optionally include a list of home network public keys for encrypting SUPIs.  If both publicLandMobileNetworkIdentifier and publicLandMobileNetworkIdentifiers are specified, precedence will be given to publicLandMobileNetworkIdentifiers.",
          "x-ms-identifiers": [
            "mcc",
            "mnc"
          ]
        },
        "serviceKey": {
          "type": "string",
          "readOnly": true,
          "description": "The mobile network resource identifier"
        },
        "endpointSecurityGroups": {
          "type": "array",
          "items": {
            "$ref": "./common.json#/definitions/EndpointSecurityGroup"
          },
          "minItems": 1,
          "uniqueItems": true,
          "description": "The list of endpoint security groups used in the mobile network.",
          "x-ms-identifiers": [
            "name"
          ]
        }
      },
      "required": [
        "publicLandMobileNetworkIdentifier"
      ]
    },
    "publicLandMobileNetworkIdentifier": {
      "type": "object",
      "properties": {
        "homeNetworkPublicKeys": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/homeNetworkPublicKey"
          },
          "minItems": 1,
          "uniqueItems": true,
          "description": "This provides a mapping to identify which public key has been used for SUPI concealment.",
          "x-ms-identifiers": [
            "keyId"
          ]
        }
      },
      "allOf": [
        {
          "$ref": "./ts29571.json#/definitions/PlmnId"
        }
      ]
    },
    "homeNetworkPublicKey": {
      "type": "object",
      "properties": {
        "keyId": {
          "type": "integer",
          "format": "int32",
          "minimum": 1,
          "maximum": 255,
          "description": "The Home Network Public Key Identifier determines which public key was used to generate the Protection Scheme Output."
        },
        "keySecretUrl": {
          "type": "string",
          "description": "The URL of the secret containing the private key, versioned or unversioned. For example: https://contosovault.vault.azure.net/secrets/mySuciPrivateKey/562a4bb76b524a1493a6afe8e536ee78."
        },
        "keyProfile": {
          "type": "string",
          "enum": [
            "A",
            "B"
          ],
          "x-ms-enum": {
            "name": "keyProfile",
            "modelAsString": true
          },
          "description": "The protection scheme that has been used to conceal the SUPI."
        }
      },
