    "api-version": "2021-01-01-alpha",
    "subscriptionId": "{subscriptionId}",
    "resourceGroupName": "rg1",
    "organization": "{organizationName}",
    "project": "{projectName}",
    "environment": "{environmentName}",
    "deploymentName": "{deploymentName}"
  },
