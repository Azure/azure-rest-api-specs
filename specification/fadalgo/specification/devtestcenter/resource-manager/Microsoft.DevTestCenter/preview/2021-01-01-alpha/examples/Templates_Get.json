{
  "parameters": {
    "api-version": "2021-01-01-alpha",
    "subscriptionId": "{subscriptionId}",
    "resourceGroupName": "rg1",
    "organization": "{organizationName}",
    "templateSource": "{templateSourceName}",
    "template": "{templateName}"
  },
  "responses": {
    "200": {
      "body": {
        "id": "/subscriptions/{subscriptionId}/resourceGroups/rg1/providers/Microsoft.DevTestCenter/organization/{organizationName}/templatesources/{templateSourceName}/templates/{templateName}",
        "name": "{templateName}",
        "type": "Microsoft.DevTestCenter/organizations/templatesources/templates",
        "properties": {
          "description": "Hello world template to deploy a basic API service",
          "type": "ARM",
          "parameters": [
            {
              "name": "app_name",
              "type": "string",
              "description": "The name of the application. This must be provided when deploying an environment with this template."
            }
          ]
        }
      }
    }
  }
}
