        "sourceConnectionInfo": {
          "description": "Connection information for source Oracle",
          "$ref": "./Common.json#/definitions/OracleConnectionInfo"
        }
      },
      "required": [
        "targetConnectionInfo",
        "sourceConnectionInfo",
        "selectedDatabases"
      ]
