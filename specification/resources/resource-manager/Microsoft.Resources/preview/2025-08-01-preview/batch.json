        },
        "headers": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "description": "Headers that are specific to this response.",
          "readOnly": true
        },
        "body": {
          "type": "object",
          "description": "The body of the response.",
          "readOnly": true
        }
