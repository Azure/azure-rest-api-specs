          },
          {
            "name": "parameters",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/BatchRequests"
            },
            "description": "Specifications for all of the requests that will be invoked as part of the batch."
          }
        ],
        "responses": {
          "202": {
            "description": "The batch was accepted and has begun processing.",
            "headers": {
              "Location": {
                "type": "string",
                "description": "URL to get status of this long-running operation."
              },
              "Azure-AsyncOperation": {
                "type": "string",
                "description": "URL to get status of this long-running operation."
              }
            },
            "schema": {
              "$ref": "../../../../../common-types/resource-management/v6/types.json#/definitions/OperationStatusResult"
            }
          },
          "200": {
            "description": "OK - Returns response details for all requests in the batch.",
            "schema": {
              "$ref": "#/definitions/BatchResponseStatus"
            }
          },
          "default": {
