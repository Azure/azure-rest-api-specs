{
  "operationId": "Translator_TextTranslation_BreakSentence",
  "title": "Returns an array of integers representing the length of sentences in a source text.",
  "parameters": {
    "Endpoint": "{Endpoint}",
    "ApiVersion": "v3.0",
    "Content-Type": "application/json",
    "body": [
      {"Text":"How are you? I am fine. What did you do today?"}
    ]
  },
  "responses": {
    "200": [
      {
        "detectedLanguage": {
          "language": "en",
          "score": 1.0
        },
        "sentLen": [
          13,
          11,
          22
        ]
      }
    ],
    "default": {
      "headers": {
        "Content-Type": "application/json",
        "x-requestid": "Request id for tracing"
      },
      "body": {
        "error": {
          "code": 400000,
          "message": "Error Message"
        }
      }
    }
  }
}
