          "url": "https://docs.microsoft.com/en-us/rest/api/billing/"
        },
        "operationId": "BillingAccounts_List",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "includeAll",
            "description": "When true, results will include Billing Accounts that the user does not have a direct role assignment on if the user has one of the following AAD roles: Global Administrator, Global Reader, Billing Administrator.",
            "type": "boolean",
            "default": false
          },
          {
            "in": "query",
            "name": "includeAllWithoutBillingProfiles",
            "description": "When true, results will include Billing Accounts that are not fully created if the user has one of the following AAD roles: Global Administrator, Global Reader, Billing Administrator.",
            "type": "boolean",
            "default": false
          },
          {
            "in": "query",
            "name": "includeDeleted",
            "description": "When true, results will include any billing accounts in a deleted state.",
            "type": "boolean",
            "default": false
          },
          {
            "in": "query",
            "name": "legalOwnerTID",
            "description": "Must be combined with legalOwnerOID, results will only include Billing Accounts for whom is legally responsible for the Billing Accounts. Optional.",
            "type": "string"
          },
          {
            "in": "query",
            "name": "legalOwnerOID",
