    },
    "ownerId": {
      "type": "string",
      "description": "Owner of the subscription",
      "x-apim-code-resourceId": "/users/{uid}",
      "x-apim-code-nillable": true
    },
    "scope": {
      "type": "string",
      "x-apim-code-resourceId": "{scope}",
      "description": "Scope like /products/{productId} or /apis or /apis/{apiId} or /."
    },
    "name": {
      "type": "string",
      "description": "Subscription name."
    },
    "state": {
