{
  "operationId": "CreateStreaming",
  "title": "Creates a chat completion for the provided prompt using streaming.",
  "parameters": {
    "endpoint": "{endpoint}",
    "api-version": "2023-10-01-preview",
    "body": {
      "messages": [
        {
          "role": "user",
          "message": "Tell me a joke"
        }
      ],
      "stream": true
    }
  },
  "responses": {
    "produces": [
      "text/event-stream"
    ],
    "200": {
      "description": "OK"
    }
  }
}
