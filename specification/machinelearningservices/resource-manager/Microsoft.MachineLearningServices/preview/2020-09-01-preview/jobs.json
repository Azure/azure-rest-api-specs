        "NotStarted",
        "Starting",
        "Provisioning",
        "Preparing",
        "Queued",
        "Running",
        "Finalizing",
        "CancelRequested",
        "Completed",
        "Failed",
        "Canceled",
        "NotResponding"
      ],
      "type": "string"
    },
    "CodeConfiguration": {
      "description": "Code configuration for a job",
      "type": "object",
      "properties": {
        "codeArtifactId": {
          "description": "The ID of the code asset.",
          "type": "string"
        },
        "startupCommand": {
          "description": "The command to execute on startup of the job.",
          "type": "string"
        }
