        "Mount",
        "Download",
        "Upload"
      ],
      "type": "string"
    },
    "DataBinding": {
      "description": "Data binding definition.",
      "type": "object",
      "properties": {
        "sourceDataReference": {
          "description": "Reference to source data artifact.",
          "type": "string"
        },
        "localReference": {
          "description": "Location of data inside the container process.",
          "type": "string"
        },
        "mode": {
          "$ref": "#/definitions/DataBindingMode"
        }
      }
    },
    "ScaleConfiguration": {
      "type": "object",
      "properties": {
