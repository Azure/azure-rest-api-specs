          {
            "in": "query",
            "name": "userId",
            "description": "User Id.",
            "type": "string"
          },
          {
            "in": "query",
            "name": "role",
            "description": "Roles that the connection with the generated token will have.",
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "in": "query",
            "name": "minutesToExpire",
            "description": "The expire time of the generated token.",
            "type": "integer",
            "format": "int32",
            "default": 60
          },
          {
            "in": "query",
            "name": "api-version",
            "description": "The version of the REST APIs.",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/ClientTokenResponse"
