import "@cadl-lang/rest";
using Cadl.Http;

namespace HealthDecisionSupportApi;

@doc("The request was accepted and a job was created.")
model SuccessfulPostJobResponse {
  @doc("The status code.")
  @statusCode
  statusCode: 202;
  ...Azure.Core.Foundations.RetryAfterHeader;

  @doc("Indicates whether the repeatable request was accepted or rejected.")
  @header("Repeatability-Result")
  @visibility("read")
  repeatabilityResult?: RepeatabilityResult;
}

@doc("""
"The result of a repeatable request (`accepted`/`rejected`), as specified in [OASIS Repeatable Requests Version 1.0](https://docs.oasis-open.org/odata/repeatable-requests/v1.0/cs01/repeatable-requests-v1.0-cs01.html#sec_RepeatabilityResult).
""")
enum RepeatabilityResult {
  Accepted: "accepted",
  Rejected: "rejected",
}
