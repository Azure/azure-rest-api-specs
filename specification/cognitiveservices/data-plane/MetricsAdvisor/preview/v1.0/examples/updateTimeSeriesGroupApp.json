{
  "parameters": {
    "endpoint": "{endpoint}",
    "Ocp-Apim-Subscription-Key": "{API key}",
    "Content-Type": "application/merge-patch+json",
    "body": {
      "displayName": "Sample Application Display Name",
      "description": "Sample Application Description",
      "showResult": "DataGrid",
      "alertable": false,
      "trainable": true,
      "inferenceable": true,
      "parameters": {
        "model": {
          "params1": 1,
          "params2": "two"
        },
        "fields": [
          {
            "help": "Parameter one tip.",
            "name": "params1",
            "type": "slider",
            "label": "Parameter One",
            "props": {
              "max": 1000,
              "min": 1,
              "show-input": true
            },
            "rules": {
              "max": 1000,
              "min": 1,
              "type": "number",
              "trigger": "blur,change"
            }
          },
          {
            "help": "Parameter two tip.",
            "name": "params2",
            "type": "input",
            "label": "Parameter Two",
            "props": {
              "placeholder": "Please input text."
            },
            "rules": {
              "required": true
            }
          }
        ]
      },
      "endpointMeta": "http://example.com:12345",
      "endpointTrain": "http://example.com:12345",
      "endpointInference": "http://example.com:12345",
      "stage": "Public",
      "status": "OK",
      "dump": {
        "target": "TSDB",
        "timestamp": "col_ts",
        "granularityName": "Custom",
        "granularityAmount": 60,
        "metrics": {
          "metricName": "SampleMetricName"
        }
      },
      "admins": [
        "admin@company.example"
      ],
      "viewers": [
        "viewer@company.example"
      ],
      "tags": [
        {
          "tag": "App Tag",
          "type": 1
        }
      ]
    }
  },
  "responses": {
    "204": {}
  }
}
