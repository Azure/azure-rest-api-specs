            "type": "object",
            "properties": {
              "usage": {
                "type": "string",
                "description": "For what this IP range is used"
              },
              "startIp": {
                "$ref": "#/definitions/IpAddress"
              },
              "endIp": {
                "$ref": "#/definitions/IpAddress"
              },
              "aksDetails": {
                "type": "object",
                "description": "Information applicable to AKS deployments",
                "properties": {
                  "subnetName": {
                    "type": "string",
                    "description": "Name of the virtual network subnet from which this IP range is allocated."
                  }
                }
              }
            }
          }
        },
        "allocatedNames": {
          "type": "array",
          "description": "Names that have been allocated for this deployment",
          "items": {
            "type": "object",
            "properties": {
              "usage": {
                "type": "string",
                "description": "For what this name is used"
              },
              "name": {
                "type": "string",
                "description": "Allocated name"
              },
              "value": {
                "type": "string",
                "description": "Value of name"
              }
            }
          }
        }
      }
    },
    "PfcpPeers": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Unique name of the remote PFCP Peer"
          },
          "fqdn": {
            "$ref": "#/definitions/Fqdn",
            "description": "Peer FQDN"
