          "ipv4Address": {
            "$ref": "#/definitions/Ipv4Address",
            "description": "Peer IPv4 Address"
          },
          "ipv6Address": {
            "$ref": "#/definitions/Ipv6Address",
            "description": "Peer IPv6 Address"
          },
          "port": {
            "type": "integer",
            "default": 8805,
            "maximum": 65536,
            "description": "Peer port.  Default is 8805"
          }
        }
      },
      "description": "A list of remote PFCP peers with which the local network function instance communicates"
    },
