    "NetworkFunctionVersionManagement": {
      "type": "object",
      "description": "Image management information for individual network functions",
      "properties": {
        "currentImageVersion": {
          "type": "string",
          "readOnly": true,
          "description": "The image version this network function is currently running"
        },
        "desiredImageVersion": {
          "type": "string",
          "readOnly": true,
          "description": "The image version to which this network function will be upgraded"
        }
      }
    },
    "SummaryOfResources": {
      "readOnly": true,
      "type": "array",
      "description": "List of objects that are present or have changed (added/delete/modified)",
      "items": {
        "type": "object",
        "properties": {
          "resourceId": {
            "$ref": "#/definitions/ReferencedResource",
            "description": "The reference to the changed object"
          },
          "action": {
            "description": "The action applied to the object.  Used when getting change lists.  Not used in the manifest response",
            "type": "string",
            "enum": [
              "Create",
              "Update",
              "Delete"
            ],
            "x-ms-enum": {
              "name": "SummaryChangeAction",
              "modelAsString": true
            }
          }
        }
      }
    },
    "BaseMetrics": {
      "type": "object",
      "description": "Base metrics of the Mobile Packet Core instance",
      "readOnly": true,
      "properties": {
        "mobilityManagement": {
          "type": "object",
          "description": "Metrics applicable to mobility management functions",
          "properties": {
            "currentSaauCount": {
              "type": "integer",
              "format": "int64",
              "description": "Gauge showing the current count of Simultaneously Attached Active Users."
            }
          }
        },
        "sessionManagement": {
          "type": "object",
          "description": "Metrics applicable to session management functions",
          "properties": {
            "currentSessionCount": {
              "type": "integer",
              "format": "int64",
              "description": "Gauge showing the current count of PDU Sessions."
            }
          }
        },
        "userPlane": {
          "type": "object",
          "description": "Metrics applicable to user plane functions",
          "properties": {
            "dataTransferred": {
              "type": "number",
              "description": "Total bidirectional data transferred (GB)"
            }
          }
        }
      }
    },
