  "definitions": {
    "ItemDetailsList": {
      "description": "List of item details.",
      "type": "object",
      "properties": {
        "value": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ItemDetails"
          }
        },
        "nextLink": {
