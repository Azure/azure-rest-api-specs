        },
        "issuerName": {
          "description": "Issuer distinguished name.",
          "type": "string",
          "readOnly": true,
          "example": "CN=CONTOSO INTERMEDIATE CA 01"
        },
        "notBeforeUtc": {
          "format": "date-time",
